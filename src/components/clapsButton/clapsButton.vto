<button
	class="clapsButton"
	title="Clap"
	aria-pressed="false"
	aria-live="polite"
>
	<span class="clapsButtonLabel">
		<span> Clap </span>
	</span>
	<span class="clapsButtonAmount">
		<span class="clapsButtonAmountAbbreviated" aria-hidden="true"
			>{{ claps }}</span
		>
		<span class="clapsButtonAmountTotal" aria-live="polite">{{ claps }}</span>
	</span>
</button>
